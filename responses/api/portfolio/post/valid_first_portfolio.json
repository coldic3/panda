{
  "@context": "/contexts/Portfolio",
  "@id": "/portfolios/@uuid@",
  "@type": "Portfolio",
  "name": "My Portfolio",
  "default": true,
  "items": [
    {
      "@id": "/portfolio-items/@uuid@",
      "@type": "PortfolioItem",
      "resource": {
        "ticker": "@string@",
        "name": "@string@"
      },
      "longQuantity": "@integer@",
      "shortQuantity": "@integer@"
    }
  ],
  "createdAt": "@datetime@",
  "updatedAt": "@datetime@"
}
