{
  "@context": "/contexts/Portfolio",
  "@id": "/portfolios/@uuid@",
  "@type": "Portfolio",
  "name": "Additional Portfolio",
  "mainResource": {
    "ticker": "USD",
    "name": "US Dollar"
  },
  "default": false,
  "items": [
    {
      "@id": "/portfolio_items/@uuid@",
      "@type": "PortfolioItem",
      "resource": {
        "ticker": "@string@",
        "name": "@string@"
      },
      "quantity": {
        "long": "@integer@",
        "short": "@integer@"
      }
    }
  ],
  "createdAt": "@datetime@",
  "updatedAt": "@datetime@"
}
