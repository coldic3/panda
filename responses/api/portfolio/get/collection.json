{
  "@context": "/contexts/Portfolio",
  "@id": "/portfolios",
  "@type": "hydra:Collection",
  "hydra:totalItems": 2,
  "hydra:member": [
    {
      "@id": "/portfolios/@uuid@",
      "@type": "Portfolio",
      "name": "Default Portfolio",
      "default": true,
      "items": [
        {
          "@id": "/portfolio-items/@uuid@",
          "@type": "PortfolioItem",
          "resource": {
            "ticker": "@string@",
            "name": "@string@"
          },
          "longQuantity": "@integer@",
          "shortQuantity": "@integer@"
        },
        {
          "@id": "/portfolio-items/@uuid@",
          "@type": "PortfolioItem",
          "resource": {
            "ticker": "@string@",
            "name": "@string@"
          },
          "longQuantity": "@integer@",
          "shortQuantity": "@integer@"
        },
        {
          "@id": "/portfolio-items/@uuid@",
          "@type": "PortfolioItem",
          "resource": {
            "ticker": "@string@",
            "name": "@string@"
          },
          "longQuantity": "@integer@",
          "shortQuantity": "@integer@"
        }
      ],
      "createdAt": "@datetime@",
      "updatedAt": "@datetime@"
    },
    {
      "@id": "/portfolios/@uuid@",
      "@type": "Portfolio",
      "name": "Additional Portfolio",
      "default": false,
      "items": [
        {
          "@id": "/portfolio-items/@uuid@",
          "@type": "PortfolioItem",
          "resource": {
            "ticker": "@string@",
            "name": "@string@"
          },
          "longQuantity": "@integer@",
          "shortQuantity": "@integer@"
        }
      ],
      "createdAt": "@datetime@",
      "updatedAt": "@datetime@"
    }
  ]
}
