{
  "@context": "/contexts/Transaction",
  "@id": "/transactions/@uuid@",
  "@type": "Transaction",
  "type": "transfer",
  "toOperation": {
    "@id": "/operations/@uuid@",
    "@type": "Operation",
    "resource": "/assets/@uuid@",
    "quantity": "@integer@.greaterThan(0)"
  },
  "adjustmentOperations": [
    {
      "@id": "/operations/@uuid@",
      "@type": "Operation",
      "resource": "/assets/@uuid@",
      "quantity": "@integer@.greaterThan(0)"
    }
  ],
  "concludedAt": "@datetime@"
}
